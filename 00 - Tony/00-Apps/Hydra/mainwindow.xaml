<dxr:DXRibbonWindow x:Class="StockSharp.Hydra.MainWindow"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:loc="clr-namespace:StockSharp.Localization;assembly=StockSharp.Localization"
                    xmlns:hydra="clr-namespace:StockSharp.Hydra"
                    xmlns:sx="http://schemas.stocksharp.com/xaml"
                    xmlns:controls="clr-namespace:StockSharp.Hydra.Controls"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"
                    xmlns:xaml="clr-namespace:Ecng.Xaml;assembly=StockSharp.Xaml"
                    xmlns:studioControls="clr-namespace:StockSharp.Studio.Controls;assembly=StockSharp.Studio.Controls"
                    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
                    Loaded="MainWindowLoaded" Title="S#.Data" Height="768" Width="1200" WindowState="Maximized"
                    UseLayoutRounding="True" DisplayShowModeSelector="True"
                    DataContext="{Binding RelativeSource={RelativeSource Self}}">
  <UIElement.CommandBindings>
    <CommandBinding Executed="ExecutedStartStopCommand" CanExecute="CanExecuteStartStopCommand"
                    Command="{x:Static hydra:MainWindow.StartStopCommand}"/>
    <CommandBinding Executed="ExecutedTaskEnabledChangedCommand" CanExecute="CanExecuteTaskEnabledChangedCommand"
                    Command="{x:Static hydra:MainWindow.TaskEnabledChangedCommand}"/>
    <CommandBinding Executed="ExecutedRemoveTaskCommand" CanExecute="CanExecuteRemoveTaskCommand"
                    Command="{x:Static hydra:MainWindow.RemoveTaskCommand}"/>
    <CommandBinding Executed="ExecutedTargetPlatformCommand" Command="{x:Static hydra:MainWindow.TargetPlatformCommand}"/>
    <CommandBinding Executed="ExecutedHelpCommand" Command="{x:Static hydra:MainWindow.HelpCommand}"/>
    <CommandBinding Executed="ExecutedAboutCommand" Command="{x:Static hydra:MainWindow.AboutCommand}"/>
    <CommandBinding Executed="QuestionCommand_OnExecuted" Command="{x:Static hydra:MainWindow.QuestionCommand}"/>
    <CommandBinding Executed="ExecutedResetSettingsCommand" CanExecute="CanExecuteResetSettingsCommand"
                    Command="{x:Static hydra:MainWindow.ResetSettingsCommand}"/>
    <CommandBinding Executed="ExecutedEraseDataCommand" CanExecute="CanExecuteEraseDataCommand"
                    Command="{x:Static hydra:MainWindow.EraseDataCommand}"/>
    <CommandBinding Executed="ExecutedSynchronizeCommand" CanExecute="CanExecuteSynchronizeCommand"
                    Command="{x:Static hydra:MainWindow.SynchronizeCommand}"/>
    <CommandBinding Executed="ExecutedExit" Command="Close"/>
    <CommandBinding Executed="ExecutedOpenPaneCommand" CanExecute="CanUILoadedCommand"
                    Command="{x:Static hydra:MainWindow.OpenPaneCommand}"/>
    <CommandBinding Executed="ExecutedImportPaneCommand" CanExecute="CanUILoadedCommand"
                    Command="{x:Static hydra:MainWindow.ImportPaneCommand}"/>
    <CommandBinding CanExecute="CanUILoadedCommand" Executed="ExecutedAnalyticsCommand"
                    Command="{x:Static hydra:MainWindow.AnalyticsCommand}"/>
    <CommandBinding Executed="ExecutedBoardsCommand" CanExecute="CanUILoadedCommand"
                    Command="{x:Static hydra:MainWindow.BoardsCommand}"/>
    <CommandBinding Executed="ExecutedMemoryStatisticsCommand"
                    Command="{x:Static hydra:MainWindow.MemoryStatisticsCommand}"/>
    <CommandBinding Executed="ExecutedAddSourcesCommand" CanExecute="CanUILoadedCommand"
                    Command="{x:Static hydra:MainWindow.AddSourcesCommand}"/>
    <CommandBinding Executed="ExecutedAddToolsCommand" CanExecute="CanUILoadedCommand"
                    Command="{x:Static hydra:MainWindow.AddToolsCommand}"/>
    <CommandBinding CanExecute="CanUILoadedCommand" Executed="ExecutedServerSettingsCommand"
                    Command="{x:Static hydra:MainWindow.ServerSettingsCommand}"/>
    <CommandBinding Executed="ExecutedCommonSettingsCommand" Command="{x:Static hydra:MainWindow.CommonSettingsCommand}"/>
    <CommandBinding Executed="ExecutedProxySettingsCommand" Command="{x:Static hydra:MainWindow.ProxySettingsCommand}"/>
    <CommandBinding CanExecute="CanUILoadedCommand" Executed="ExecutedEditUsersCommand"
                    Command="{x:Static hydra:MainWindow.EditUsersCommand}"/>
    <CommandBinding CanExecute="CanUILoadedCommand" Executed="ExecutedSubscriptionsCommand"
                    Command="{x:Static hydra:MainWindow.SubscriptionsCommand}"/>
    <CommandBinding CanExecute="CanUILoadedCommand" Executed="ExecutedSecurityMappingCommand"
                    Command="{x:Static hydra:MainWindow.SecurityMappingCommand}"/>
    <CommandBinding CanExecute="CanExecuteSimulatorSettingsCommand" Executed="ExecutedSimulatorSettingsCommand"
                    Command="{x:Static hydra:MainWindow.SimulatorSettingsCommand}"/>
    <CommandBinding Executed="BugReportCommand_OnExecuted" Command="{x:Static hydra:MainWindow.BugReportCommand}"/>
  </UIElement.CommandBindings>
  <xaml:BusyIndicator x:Name="BusyIndicator" BusyContentTemplate="{StaticResource BusyContentTemplate}">
    <dxb:BarManager>
      <Grid>
        <Grid.RowDefinitions>
          <RowDefinition Height="Auto"/>
          <RowDefinition Height="*"/>
          <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <dxr:RibbonControl x:Name="Ribbon" RibbonStyle="Office2010" ShowApplicationButton="False" ToolbarShowMode="Hide">
          <dxr:RibbonDefaultPageCategory>
            <dxr:RibbonPage x:Name="RibbonCommonTab" Caption="{x:Static loc:LocalizedStrings.Common}">
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.XamlStr166}">
                <dxb:BarButtonItem x:Name="StartStopBigBtn" Content="{x:Static loc:LocalizedStrings.Str2421}"
                                   ToolTip="{x:Static loc:LocalizedStrings.Str2421}"
                                   Command="{x:Static hydra:MainWindow.StartStopCommand}" LargeGlyph="{sx:ThemedIcons Key=Start}"/>
                <dxb:BarSubItem Content="{x:Static loc:LocalizedStrings.Str3229}" ToolTip="{x:Static loc:LocalizedStrings.Str3229}"
                                LargeGlyph="{sx:ThemedIcons Key=Add}">
                  <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.XamlStr227}"
                                     Command="{x:Static hydra:MainWindow.AddSourcesCommand}" Glyph="{sx:ThemedIcons Key=Download}"/>
                  <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Tools}" Command="{x:Static hydra:MainWindow.AddToolsCommand}"
                                     Glyph="{sx:ThemedIcons Key=Check}"/>
                </dxb:BarSubItem>
                <dxb:BarSplitButtonItem x:Name="SettingsBtn" Content="{x:Static loc:LocalizedStrings.Settings}"
                                        ToolTip="{x:Static loc:LocalizedStrings.Settings}"
                                        Command="{x:Static hydra:MainWindow.CommonSettingsCommand}" ActAsDropDown="False"
                                        LargeGlyph="{sx:ThemedIcons Key=Settings}">
                  <dxb:BarSplitButtonItem.PopupControl>
                    <dxb:PopupMenu>
                      <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.NetworkSettings}"
                                         Command="{x:Static hydra:MainWindow.ProxySettingsCommand}" Glyph="{sx:ThemedIcons Key=Lan}"/>
                      <dxb:BarItemSeparator/>
                      <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.XamlStr610}"
                                         Command="{x:Static hydra:MainWindow.TargetPlatformCommand}" Glyph="{sx:ThemedIcons Key=Worlwide}"/>
                      <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str1520}"
                                         Command="{x:Static hydra:MainWindow.ResetSettingsCommand}" Glyph="{sx:ThemedIcons Key=Refresh}"/>
                    </dxb:PopupMenu>
                  </dxb:BarSplitButtonItem.PopupControl>
                </dxb:BarSplitButtonItem>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Securities}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Securities}"
                                   Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Securities}"
                                   LargeGlyph="{sx:ThemedIcons Key=Certificate}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str2831}" Command="{x:Static hydra:MainWindow.BoardsCommand}"
                                   LargeGlyph="{sx:ThemedIcons Key=Bank}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.MarketData}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Ticks}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Ticks}" LargeGlyph="{sx:ThemedIcons Key=Deal}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.MarketDepths}"
                                   Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Depths}" LargeGlyph="{sx:ThemedIcons Key=UpDown}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Candles}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Candles}" LargeGlyph="{sx:ThemedIcons Key=CandleStick}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.OrderLog}"
                                   Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.OrderLog}" LargeGlyph="{sx:ThemedIcons Key=Notepad}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Level1}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Level1}" LargeGlyph="{sx:ThemedIcons Key=NumberOne}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.News}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.News}" LargeGlyph="{sx:ThemedIcons Key=Telegram}"/>
                <dxb:BarItemSeparator/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Transactions}"
                                   Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Transactions}" LargeGlyph="{sx:ThemedIcons Key=Deal}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str972}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.PositionChanges}" LargeGlyph="{sx:ThemedIcons Key=Money}"/>
                <dxb:BarItemSeparator/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str1981}" Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Indicator}" LargeGlyph="{sx:ThemedIcons Key=LineChart}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.OptionDesk}"
                                   Command="{x:Static hydra:MainWindow.OpenPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.OptionDesk}" LargeGlyph="{sx:ThemedIcons Key=Table}"/>
                <dxb:BarItemSeparator/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.XamlStr786}"
                                   Command="{x:Static hydra:MainWindow.EraseDataCommand}" LargeGlyph="{sx:ThemedIcons Key=Eraser}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Help2}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Help}" Command="{x:Static hydra:MainWindow.HelpCommand}"
                                   LargeGlyph="{sx:ThemedIcons Key=Help}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.ReportProblem}"
                                   Command="{x:Static hydra:MainWindow.BugReportCommand}" LargeGlyph="{sx:ThemedIcons Key=BugReport}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.ContactUs}"
                                   Command="{x:Static hydra:MainWindow.QuestionCommand}" LargeGlyph="{sx:ThemedIcons Key=Chat}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.About}" Command="{x:Static hydra:MainWindow.AboutCommand}"
                                   LargeGlyph="{sx:ThemedIcons Key=Home}"/>
              </dxr:RibbonPageGroup>
            </dxr:RibbonPage>
            <dxr:RibbonPage x:Name="RibbonImportTab" Caption="{x:Static loc:LocalizedStrings.Import}">
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Synchronize}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Synchronize}"
                                   ToolTip="{x:Static loc:LocalizedStrings.XamlStr652}"
                                   Command="{x:Static hydra:MainWindow.SynchronizeCommand}" LargeGlyph="{sx:ThemedIcons Key=Refresh}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Import}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Securities}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Securities}"
                                   LargeGlyph="{sx:ThemedIcons Key=Certificate}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Ticks}" Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Ticks}" LargeGlyph="{sx:ThemedIcons Key=Deal}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.MarketDepths}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Depths}" LargeGlyph="{sx:ThemedIcons Key=UpDown}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Candles}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Candles}" LargeGlyph="{sx:ThemedIcons Key=CandleStick}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.OrderLog}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.OrderLog}" LargeGlyph="{sx:ThemedIcons Key=Notepad}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Level1}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Level1}" LargeGlyph="{sx:ThemedIcons Key=NumberOne}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.News}" Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.News}" LargeGlyph="{sx:ThemedIcons Key=Telegram}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Transactions}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.Transactions}" LargeGlyph="{sx:ThemedIcons Key=Deal}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str972}"
                                   Command="{x:Static hydra:MainWindow.ImportPaneCommand}"
                                   CommandParameter="{x:Static hydra:PaneNames.PositionChanges}" LargeGlyph="{sx:ThemedIcons Key=Money}"/>
              </dxr:RibbonPageGroup>
            </dxr:RibbonPage>
            <dxr:RibbonPage x:Name="RibbonAnalyticsTab" Caption="{x:Static loc:LocalizedStrings.Analytics}">
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.XamlStr662}">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Analytics}"
                                   Command="{x:Static hydra:MainWindow.AnalyticsCommand}" LargeGlyph="{sx:ThemedIcons Key=Function}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup x:Name="RibbonAnalyticsGroup" Caption="{x:Static loc:LocalizedStrings.Common}" IsEnabled="False">
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Str2421}" ToolTip="{x:Static loc:LocalizedStrings.Str2421}"
                                   LargeGlyph="{sx:ThemedIcons Key=Start}" Command="{Binding StartCommand}"/>
                <dxb:BarItemLinkSeparator/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Undo}" KeyGesture="CTRL+Z"
                                   LargeGlyph="{sx:ThemedIcons Key=Undo}" Command="{Binding UndoCommand}"/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Redo}" KeyGesture="CTRL+Y"
                                   LargeGlyph="{sx:ThemedIcons Key=Redo}" Command="{Binding RedoCommand}"/>
                <dxb:BarItemLinkSeparator/>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.References}" LargeGlyph="{sx:ThemedIcons Key=Dll}"
                                   Command="{Binding ReferencesCommand}"/>
              </dxr:RibbonPageGroup>
            </dxr:RibbonPage>
            <dxr:RibbonPage x:Name="RibbonServerTab" Caption="{x:Static loc:LocalizedStrings.Str2212}">
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Settings}">
                <dxb:BarEditItem EditValueChanged="StartStopServerBtn_EditValueChanged" x:Name="StartStopServerBtn"
                                 ToolTip="{x:Static loc:LocalizedStrings.Str2421}">
                  <dxb:BarEditItem.EditSettings>
                    <dxe:ToggleSwitchEditSettings/>
                  </dxb:BarEditItem.EditSettings>
                </dxb:BarEditItem>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.Settings}"
                                   Command="{x:Static hydra:MainWindow.ServerSettingsCommand}"
                                   LargeGlyph="{sx:ThemedIcons Key=Settings}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup Caption="{x:Static loc:LocalizedStrings.Str3416}">
                <dxb:BarButtonItem x:Name="CredentialsButton" Content="{x:Static loc:LocalizedStrings.Users}"
                                   Command="{x:Static hydra:MainWindow.EditUsersCommand}" IsEnabled="False"
                                   LargeGlyph="{sx:ThemedIcons Key=Profile}"/>
                <dxb:BarButtonItem x:Name="SubscriptionsButton" Content="{x:Static loc:LocalizedStrings.Subscriptions}"
                                   Command="{x:Static hydra:MainWindow.SubscriptionsCommand}" IsEnabled="False"
                                   LargeGlyph="{sx:ThemedIcons Key=Subscribe}"/>
                <dxb:BarButtonItem x:Name="MappingButton" Content="{x:Static loc:LocalizedStrings.SecuritiesAndConnections}"
                                   Command="{x:Static hydra:MainWindow.SecurityMappingCommand}" LargeGlyph="{sx:ThemedIcons Key=Sort}"/>
              </dxr:RibbonPageGroup>
              <dxr:RibbonPageGroup x:Name="SimulatorGroup" Caption="{x:Static loc:LocalizedStrings.Str1209}">
                <dxb:BarEditItem EditValueChanged="StartStopSimulatorBtn_EditValueChanged" x:Name="StartStopSimulatorBtn"
                                 ToolTip="{x:Static loc:LocalizedStrings.Str2421}">
                  <dxb:BarEditItem.EditSettings>
                    <dxe:ToggleSwitchEditSettings/>
                  </dxb:BarEditItem.EditSettings>
                </dxb:BarEditItem>
                <dxb:BarButtonItem Content="{x:Static loc:LocalizedStrings.SimulatorSettings}"
                                   Command="{x:Static hydra:MainWindow.SimulatorSettingsCommand}" LargeGlyph="{sx:ThemedIcons Key=Game}"/>
              </dxr:RibbonPageGroup>
            </dxr:RibbonPage>
          </dxr:RibbonDefaultPageCategory>
        </dxr:RibbonControl>
        <dxdo:DockLayoutManager LayoutItemActivated="DockingManager_OnLayoutItemActivated"
                                DockItemActivated="DockingManager_OnDockItemActivated"
                                DockItemClosed="DockingManager_OnDockItemClosed" x:Name="DockingManager" Grid.Row="1"
                                FloatingMode="Desktop" DockingStyle="VS2010" ClosingBehavior="ImmediatelyRemove"
                                FloatingDocumentContainer="DocumentHost">
          <dxdo:LayoutGroup x:Name="Root" TabContentCacheMode="CacheAllTabs">
            <dxdo:LayoutGroup Orientation="Vertical" ItemWidth="3*">
              <dxdo:LayoutGroup Orientation="Horizontal" ItemHeight="3*">
                <dxdo:LayoutGroup DestroyOnClosingChildren="False" ItemWidth="200">
                  <dxdo:LayoutPanel x:Name="SolutionExplorerPanel" Caption="{x:Static loc:LocalizedStrings.Sources}" AllowClose="False">
                    <controls:TaskNavigator x:Name="NavigationBar" TaskSelected="NavigationBar_OnTaskSelected"
                                            DoubleClick="NavigationBar_OnDoubleClick"/>
                  </dxdo:LayoutPanel>
                </dxdo:LayoutGroup>
                <dxdo:LayoutGroup ItemWidth="3*" DestroyOnClosingChildren="False">
                  <dxdo:DocumentGroup x:Name="DocumentHost" TabContentCacheMode="CacheAllTabs"
                                      AutoScrollOnOverflow="PartiallyVisibleItem" DestroyOnClosingChildren="False"
                                      ClosePageButtonShowMode="InAllTabPageHeaders"/>
                </dxdo:LayoutGroup>
              </dxdo:LayoutGroup>
            </dxdo:LayoutGroup>
          </dxdo:LayoutGroup>
          <dxdo:DockLayoutManager.AutoHideGroups>
            <dxdo:AutoHideGroup DockType="Bottom">
              <dxdo:LayoutPanel x:Name="LogToolWindow" Caption="{x:Static loc:LocalizedStrings.Str3237}" AllowClose="False">
                <studioControls:LogManagerPanel Name="LogsPanel" ShowStrategies="False"/>
              </dxdo:LayoutPanel>
            </dxdo:AutoHideGroup>
          </dxdo:DockLayoutManager.AutoHideGroups>
        </dxdo:DockLayoutManager>
        <studioControls:StatusBarPanel x:Name="StatusBarPanel" Grid.Row="2"/>
      </Grid>
    </dxb:BarManager>
  </xaml:BusyIndicator>
</dxr:DXRibbonWindow>