<local:BaseSubscriptionStudioControl x:Class="StockSharp.Studio.Controls.OrderLogPanel"
                                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                     xmlns:local="clr-namespace:StockSharp.Studio.Controls"
                                     xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
                                     xmlns:sx="http://schemas.stocksharp.com/xaml">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="Auto"/>
      <RowDefinition Height="*"/>
    </Grid.RowDefinitions>
    <dxb:BarManager x:Name="BarManager" Grid.Row="0">
      <dxb:BarManager.Bars>
        <dxb:Bar Caption="{sx:Localization Main}">
          <dxb:BarButtonItem ItemClick="Filter_OnClick" ToolTipService.ShowOnDisabled="True" ToolTip="{sx:Localization Filter}"
                             Glyph="{sx:ThemedIcons Key=Filter}"/>
        </dxb:Bar>
      </dxb:BarManager.Bars>
    </dxb:BarManager>
    <sx:OrderLogGrid x:Name="OrderLogGrid" Grid.Row="1"/>
  </Grid>
</local:BaseSubscriptionStudioControl>