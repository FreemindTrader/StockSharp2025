                            <Border
                                x:Name="PART_GridOrderText"
                                Grid.Column="1"
                                Background="{TemplateBinding Control.Background}"
                                BorderBrush="{TemplateBinding a:ActiveOrderAnnotation.Stroke}"
                                BorderThickness="1,1,0,1"
                                CornerRadius="1,0,0,1">
                                <TextBlock
                                    x:Name="PART_OrderText"
                                    Margin="0,-1,0,1"
