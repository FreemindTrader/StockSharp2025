                            </StackPanel>
                            <Grid
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                s:Device.SnapsToDevicePixels="True">
                                <s:PolarAxisPanel
                                    x:Name="PART_AxisCanvas"
                                    AxisAlignment="{TemplateBinding s:AxisBase.AxisAlignment}"
                                    DrawLabels="{TemplateBinding s:AxisBase.DrawLabels}"
                                    DrawMajorTicks="{TemplateBinding s:AxisBase.DrawMajorTicks}"
                                    DrawMinorTicks="{TemplateBinding s:AxisBase.DrawMinorTicks}"
                                    IsHitTestVisible="False"
                                    IsLabelCullingEnabled="{TemplateBinding s:AxisBase.IsLabelCullingEnabled}"
                                    MajorTickLineStyle="{TemplateBinding s:AxisBase.MajorTickLineStyle}"
                                    MinorTickLineStyle="{TemplateBinding s:AxisBase.MinorTickLineStyle}"
                                    s:Device.SnapsToDevicePixels="True"
                                    s:PolarPanel.ShouldCopyThicknessToParent="True">
                                    <s:PolarPanel
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        IsReversedOrder="True"
                                        s:Device.SnapsToDevicePixels="True"
                                        ShouldCopyThicknessToParent="True">
                                        <Grid x:Name="PART_LabelsCanvas" s:Device.SnapsToDevicePixels="True">
                                            <s:PolarTickLabelAxisCanvas
                                                HorizontalAlignment="Stretch"
                                                VerticalAlignment="Stretch"
                                                Background="Transparent"
                                                ClipToBounds="False"
                                                IsLabelCullingEnabled="{TemplateBinding s:AxisBase.IsLabelCullingEnabled}"
                                                s:Device.SnapsToDevicePixels="True"
                                                s:PolarPanel.ShouldCopyThicknessToParent="True"
                                                s:PolarPanel.Thickness="{Binding MaxChildSize, RelativeSource={RelativeSource Self}, Mode=OneWay}" />
                                            <s:PolarTickLabelAxisCanvas
                                                HorizontalAlignment="Stretch"
                                                VerticalAlignment="Stretch"
                                                Background="Transparent"
                                                ClipToBounds="False"
                                                IsLabelCullingEnabled="{TemplateBinding s:AxisBase.IsLabelCullingEnabled}"
                                                s:Device.SnapsToDevicePixels="True"
                                                s:PolarPanel.ShouldCopyThicknessToParent="True"
                                                s:PolarPanel.Thickness="{Binding MaxChildSize, RelativeSource={RelativeSource Self}, Mode=OneWay}"
                                                Visibility="Collapsed" />
                                        </Grid>
                                        <Border s:PolarPanel.Thickness="{Binding AxisLabelToTickIndent, RelativeSource={RelativeSource AncestorType=s:PolarAxisPanel}, Mode=OneWay}" />
                                        <Image
                                            x:Name="PART_AxisBitmapImage"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            s:Device.SnapsToDevicePixels="True"
                                            s:PolarPanel.Thickness="{Binding MajorTickSize, RelativeSource={RelativeSource AncestorType=s:PolarAxisPanel}, Mode=OneWay}"
                                            Stretch="Fill" />
                                    </s:PolarPanel>
                                </s:PolarAxisPanel>
                            </Grid>
                            <Ellipse
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Fill="Transparent"
                                IsHitTestVisible="False"
                                s:PolarPanel.Thickness="{Binding StrokeThickness, RelativeSource={RelativeSource Self}}"
